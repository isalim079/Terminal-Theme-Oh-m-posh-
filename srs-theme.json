{
  
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "foreground": "#00c0e4",
            "style": "plain",
            "template": "@SRS",
            "type": "session"
          },
          {
            "_commentbackground": "#18354c",
           
            "_commentforeground": "#ffc107",
            "foreground": "#dc4598",
            "leading_diamond": "\ue0b6",
            "properties": {
              "style": "folder"
            },
            "style": "plain",
            "template": " \ue5ff {{ .Path }} ",
            "_commenttemplate": " \ue5ff {{ .Path }} ",
            "trailing_diamond": "\ue0b0",
            "type": "path"
          },
          {
            
            "_commentbackground": "#18354c",
            "foreground": "#00c0e4",
            "powerline_symbol": "\ue0b0",
            "properties": {
              "fetch_stash_count": true,
              "fetch_upstream_icon": true
            },
            "style": "plain",
            
            "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
            "type": "git"
          },
          {
            "background": "#ffc107",
            "foreground": "#18354c",
            "powerline_symbol": "\ue0b0",
            "style": "powerline",
            "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
            "type": "python"
          },
          {
            "background": "#ffc107",
            "foreground": "#18354c",
         
            "powerline_symbol": "\ue0b0",
            "style": "powerline",
            "template": " \uf0e7 ",
            "type": "root"
          }
          
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#56B6C2",
            "foreground_templates": [
              "{{ if gt .Code 0 }}red{{ end }}"
            ],
            "properties": {
              "always_enabled": true
            },
            "style": "powerline",
            "template": "  â†³",
            "type": "status"
          }
        ],
        "type": "prompt"
      }
    ],
    "final_space": true,
    "version": 2
  }
  